        <PackageReference Include="Horse.Mq.Data" Version="5.0.10" />
        <PackageReference Include="Horse.Mvc" Version="5.0.3" />
        <PackageReference Include="Horse.Mvc.Auth.Jwt" Version="5.0.3" />
        <PackageReference Include="Horse.Protocols.Http" Version="5.0.3" />
        <PackageReference Include="Horse.Protocols.WebSocket" Version="5.0.6" />
        <PackageReference Include="Horse.Server" Version="5.0.1" />
        <PackageReference Include="Horse.WebSocket.Models" Version="5.0.6" />
    </ItemGroup>

    <ItemGroup>
        <Folder Include="Web" />
    </ItemGroup>

