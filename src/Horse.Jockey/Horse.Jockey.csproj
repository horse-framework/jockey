<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Horse.Jockey</Title>
        <AssemblyVersion>6.3.1</AssemblyVersion>
        <FileVersion>6.3.1</FileVersion>
        <PackageVersion>6.3.1</PackageVersion>
        <Authors>Mehmet Helvacıköylü;Emre Hızlı</Authors>
        <Description>Administration Portal for Horse Messaging</Description>
        <PackageProjectUrl>https://github.com/horse-framework/jockey</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/horse-framework/jockey/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/horse-framework/jockey</RepositoryUrl>
        <LangVersion>10</LangVersion>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DocumentationFile>bin\Release\Horse.Jockey.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <DocumentationFile>bin\Debug\Horse.Jockey.xml</DocumentationFile>
    </PropertyGroup>

