<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Horse.Jockey</Title>
        <AssemblyVersion>7.2.0</AssemblyVersion>
