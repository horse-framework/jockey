        <PackageReference Include="Horse.Server" Version="6.0.1" />
        <PackageReference Include="Horse.WebSocket.Protocol" Version="6.2.2" />
        <PackageReference Include="Horse.WebSocket.Server" Version="6.2.2" />
    </ItemGroup>

