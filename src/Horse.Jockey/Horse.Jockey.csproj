        <PackageReference Include="Horse.Protocols.Http" Version="5.0.4" />
        <PackageReference Include="Horse.Protocols.WebSocket" Version="6.0.2" />
        <PackageReference Include="Horse.Server" Version="5.0.2" />
        <PackageReference Include="Horse.WebSocket.Models" Version="6.0.2" />
    </ItemGroup>

    <ItemGroup>
        <Folder Include="Web" />
    </ItemGroup>

