<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Horse.Jockey</Title>
        <AssemblyVersion>6.6.3</AssemblyVersion>
        <FileVersion>6.6.3</FileVersion>
        <PackageVersion>6.6.3</PackageVersion>
        <Authors>Mehmet Helvacıköylü;Emre Hızlı</Authors>
        <Description>Administration Portal for Horse Messaging</Description>
        <PackageProjectUrl>https://github.com/horse-framework/jockey</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/horse-framework/jockey/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/horse-framework/jockey</RepositoryUrl>
        <LangVersion>10</LangVersion>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
